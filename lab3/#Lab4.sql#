DESCRIBE employee;
SELECT dependent_name FROM dependent WHERE relationship like 'Daughter' OR relationship LIKE 'Spouse';
select pname FROM project WHERE plocation IS NULL;
SELECT fname, lname, dname FROM employee INNER JOIN department m ON mgrssn=ssn;
SELECT DISTINCT fname, lname FROM employee INNER JOIN works_on ON ssn=essn WHERE hours < 18;
SELECT fname, lname from employee where lname like 'W%';
SELECT sname FROM salespeople WHERE city IN ('San Jose', 'Barcelona');
SELECT customers.cname, orders.amt FROM orders INNER JOIN customers ON orders.cnum=customers.cnum WHERE amt between 1500 and 5000;
SELECT COUNT(*)FROM orders;
SELECT AVG(amt) "tiltttty" FROM orders;
